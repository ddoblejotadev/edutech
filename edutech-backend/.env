# Configuración específica para EduTech Backend
# Este archivo debe ser usado por el Maven wrapper

# Forzar Java 17
JAVA_HOME_17_FORCED=true
MAVEN_COMPILER_SOURCE=17
MAVEN_COMPILER_TARGET=17
MAVEN_COMPILER_RELEASE=17

# Configuración específica para Lombok
LOMBOK_VERSION=1.18.32
ANNOTATION_PROCESSOR_ENABLED=true

# Configuración de encoding
PROJECT_ENCODING=UTF-8
MAVEN_OPTS=-Dfile.encoding=UTF-8 -Duser.timezone=UTC

# Spring Boot
SPRING_PROFILES_ACTIVE=dev